#include <iostream>
using namespace std;

bool primo (int x){
   for(int i=2;i<x;i++){
           if(x%i==0){
               return false;
           }else{
               return true;
           }
    }
}

int main()
{
    if(primo(8))
      cout<<"é primo";
     else cout<<"não é primo";

    return 0;
}
